{

  "table":[
      "$db.jiaorder.unit"
  ],

  "data":{
      "unit.id":"$seq('jiaorder','unit','{$session.service_id}')",
      "unit.order_num":"$data.unit.id",
      "unit.add_datetime":"$datetime('yyyy-mm-dd hh:mm:ss')",
      "unit.service_id":"{$SESSION.SERVICE_ID}",
      "unit.sate":"-1"
  },

  "validate":{
      "def":{
        "unit.type":"1",
        "unit.class":"1000"
      },
      "isNull":{
          "unit.name":"名称不能为空",
          "unit.intro":"描述不能为空",
          "$SESSION.SERVICE_ID":"得到不您的系统ID,您无法使用此系统"
      },
      "isNum":{
          "unit.state":"状态必须为数字",
          "type":"操作类型必须为数字"
      }
  },

  "execute":{
      "add":{
          "DB":"jiaorder",
          "CACHE":"unit.id,unit.service_id",
          "CACHE_UPDATE":"true",
          "CACHE_RELOAD":"true",
          "SQL":"jiaorder:INSERT INTO unit ({$ALL_COLUMN}) values ({$DATA})",
          "IS_BATCH":"false",
          "IS_ROLLBACK":"false"
      }
  },

  "return":{
    "url":"",
    "param":[
      "unit.id",
      "unit.name",
      "unit.intro",
      "unit.add_datetime"
    ]
  },

  "aop":{
    "com.bubu.test.aaa":"",
    "com.bubu.test.aaa":""
  }


}